generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("NEXT_PUBLIC_DATABASE_URL")
}

model Influencer {
  id                 String  @id @unique
  analysis           String  

  created_at DateTime  @default(now())
  updated_at DateTime  @default(now()) @updatedAt
}